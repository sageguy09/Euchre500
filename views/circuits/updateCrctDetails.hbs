<h1 class="allHeader">Edit Circuit Details</h1>
<container class="circuitsMain" "clearfix">
    <container class="circuitBoard" id="circuitScore">
        <div aria-label="circuit"
            aria-describedby="caption" class="table" role="grid"
        >
          <form method="POST" id="updateDetails" action="/sitemain/circuits/{{circuit._id}}?_method=PUT">
            <h1 class="head">Edit Circuit: {{circuit.crctName}} Details</h1>
            <div class="tr" role="row">
        
                <div class="th" role="columnheader">
                Players
                </div>
                <div class="th" role="columnheader">
                Score
                </div>
                <div class="th" role="columnheader">
                Loaners
                </div>
                <div class="th" role="columnheader">
                Euchres
                </div>
                <div class="th" role="columnheader">
                Marches
                </div>
                <div class="th" role="columnheader">
                Wins
                </div>
                <div class="th" role="columnheader">
                Losses
                </div>
                <div class="th" role="columnheader">
                Total
                </div>
            </div>
            <div class="tr" role="row" >
                <div class="th" role="rowheader" id="nameP1">
                <input list="players" name="playersDropdown" >
                    <datalist id="players" >
                        {{#each players}}
                        <option value="{{playerAlias}}">
                        {{/each}}
                    </datalist>
                </div>
                <input class="td" role="gridcell" type="number" id="scoreP1" value="{{circuit.scoreP1}}" name="scoreP1">
                 <input class="td" role="gridcell" type="number" id="loanerP1" value="{{circuit.loanerP1}}" name="loanerP1">
                  <input class="td" role="gridcell" type="number" id="euchreP1" value="{{circuit.euchreP1}}" name="euchreP1">
                   <input class="td" role="gridcell" type="number" id="marchP1" value="{{circuit.marchP1}}" name="marchP1">
                    <input class="td" role="gridcell" type="number" id="winsP1" value="{{circuit.winsP1}}" name="winsP1">
                     <input class="td" role="gridcell" type="number" id="lossP1" value="{{circuit.lossP1}}" name="lossP1">
                        <input class="td" role="gridcell" type="number" id="totalP1" value="{{circuit.totalP1}}" name="totalP1"> 
            </div>
            <div class="tr" role="row">
                <div class="th" role="rowheader" id="nameP2">
                <input list="players" name="playersDropdown" >
                    <datalist id="players" >
