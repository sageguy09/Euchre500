                        {{/each}}
                    </datalist>
                </div>
                <input class="td" role="gridcell" type="number" id="scoreP2" value="{{circuit.scoreP2}}" name="scoreP2">
                 <input class="td" role="gridcell" type="number" id="loanerP2" value="{{circuit.loanerP2}}" name="loanerP2">
                  <input class="td" role="gridcell" type="number" id="euchreP2" value="{{circuit.euchreP2}}" name="euchreP2">
                   <input class="td" role="gridcell" type="number" id="marchP2" value="{{circuit.marchP2}}" name="marchP2">
                    <input class="td" role="gridcell" type="number" id="winsP2" value="{{circuit.winsP2}}" name="wins2">
                     <input class="td" role="gridcell" type="number" id="lossP2" value="{{circuit.lossP2}}" name="lossP2">
                        <input class="td" role="gridcell" type="number" id="totalP2" value="{{circuit.totalP2}}" name="totalP2"> 
            </div>
            <div class="tr" role="row">
                <div class="th" role="rowheader" id="nameP3">
                <input list="players" name="playersDropdown" >
                    <datalist id="players" >
                        {{#each players}}
                        <option value="{{playerAlias}}">
                        {{/each}}
                    </datalist>
                </div>
                <input class="td" role="gridcell" type="number" id="scoreP3" value="{{circuit.scoreP3}}" name="scoreP3">
                 <input class="td" role="gridcell" type="number" id="loanerP3" value="{{circuit.loanerP3}}" name="loanerP3">
                  <input class="td" role="gridcell" type="number" id="euchreP3" value="{{circuit.euchreP3}}" name="euchreP3">
                   <input class="td" role="gridcell" type="number" id="marchP3" value="{{circuit.marchP3}}" name="marchP3">
                    <input class="td" role="gridcell" type="number" id="winsP3" value="{{circuit.winsP3}}" name="winsP3">
                     <input class="td" role="gridcell" type="number" id="lossP3" value="{{circuit.lossP3}}" name="lossP3">
                        <input class="td" role="gridcell" type="number" id="totalP3" value="{{circuit.totalP3}}" name="totalP3"> 
            </div>
            <div class="tr" role="row">
                <div class="th" role="rowheader" id="nameP4">
                <input list="players" name="playersDropdown" >
                    <datalist id="players" >
                        {{#each players}}
                        <option value="{{playerAlias}}">
                        {{/each}}
                    </datalist>
                </div>
                <input class="td" role="gridcell" type="number" id="scoreP4" value="{{circuit.scoreP4}}" name="scoreP4">
                 <input class="td" role="gridcell" type="number" id="loanerP4" value="{{circuit.loanerP4}}"name="loanerP4">
                  <input class="td" role="gridcell" type="number" id="euchreP4" value="{{circuit.euchreP4}}" name="euchreP4">
                   <input class="td" role="gridcell" type="number" id="marchP4" value="{{circuit.marchP4}}" name="marchP4">
                    <input class="td" role="gridcell" type="number" id="winsP4" value="{{circuit.winsP4}}" name="winsP4">
                     <input class="td" role="gridcell" type="number" id="lossP4" value="{{circuit.lossP4}}" name="lossP4">
                        <input class="td" role="gridcell" type="number" id="totalP4" value="{{circuit.totalP4}}" name="totalP4"> 
            </div>
            <input type="submit" value="Update Circuit" />
            <input type="submit" value="Cancel" formmethod="GET" formaction="/sitemain/circuits/{{circuit._id}}">
            {{!-- <input type="submit" value="Delete"  formaction="/sitemain/circuits/{{circuit._id}}?_method=DELETE"> --}}
            <input type="submit"  form="deleteCircuit" value="Delete Circuit" onclick="confirmDeleteCircuit();" >
          </form>
          <form id="deleteCircuit" method="POST" action="/sitemain/circuits/{{circuit._id}}?_method=Delete">
          
            </form>
        </div>
    </container>
