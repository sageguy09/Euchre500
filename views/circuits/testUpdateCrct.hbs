<h1>Test Resume Circuit {{circuit._id}}</h1>

<div class='circuitDiv' id='existingCircuit'>
<form method="POST" action="/sitemain/circuits/{{circuit._id}}?_method=PUT">

  <!--
    Template for creating input form for a string object key called stringMember
    NOTE: for numeric input fields use `type="number"`
  --->
  {{!-- circuit name/id --}}
  <label for="crctName">Circuit Name</label>
  <input type="text" value="{{circuit.crctName}}" name="crctName" />
{{!-- player names --}}
  <label for="crctCreated">Created By</label>
  <input type="text" value="{{circuit.crctCreatedBy}}" name="crctCreatedBy" />

  <label for="nameP1">Player Name 1</label>
  <input type="text" value="{{circuit.nameP1}}" name="nameP1" />

   <label for="nameP2">Player Name 2</label>
  <input type="text" value="{{circuit.nameP2}}"name="nameP2" />

   <label for="nameP3">Player Name 3</label>
  <input type="text" value="{{circuit.nameP3}}" name="nameP3" />

   <label for="nameP4">Player Name 4</label>
  <input type="text" value="{{circuit.nameP4}}" name="nameP4" />
{{!-- score fields --}}
   <label for="scoreP1">Player 1 Score</label>
  <input type="text" value="{{circuit.scoreP1}}" name="scoreP1" />

  <label for="scoreP2">Player 2 Score</label>
  <input type="text" value="{{circuit.scoreP2}}" name="scoreP2" />

    <label for="scoreP3">Player 3 Score</label>
  <input type="text" value="{{circuit.scoreP3}}" name="scoreP3" />

    <label for="scoreP4">Player 4 Score</label>
  <input type="text" value="{{circuit.scoreP4}}" name="scoreP4" />
{{!-- march --}}
    <label for="marchP1">Player 1 Marches</label>
  <input type="text" value="{{circuit.marchP1}}" name="marchP1" />

    <label for="marchP2">Player 2 Marches</label>
  <input type="text" value="{{circuit.marchP2}}" name="marchP2" />

  <label for="marchP3">Player 3 Marches</label>
  <input type="text" value="{{circuit.marchP3}}" name="marchP3" />

  <label for="marchP4">Player 4 Marches</label>
  <input type="text" value="{{circuit.marchP4}}" name="marchP4" />
  {{!-- euchre --}}
    <label for="euchreP1">Player 1 Euchres</label>
  <input type="text" value="{{circuit.euchreP1}}" name="euchreP1" />

    <label for="euchreP2">Player 2 Euchres</label>
  <input type="text" value="{{circuit.euchreP2}}" name="euchreP2" />

    <label for="euchreP3">Player 3 Euchres</label>
  <input type="text" value="{{circuit.euchreP3}}" name="euchreP3" />

     <label for="euchreP4">Player 4 Euchres</label>
  <input type="text" value="{{circuit.euchreP4}}" name="euchreP4" />
