    <label for="loanerP1">Player 1 Loaners</label>
  <input type="text" value="{{circuit.loanerP1}}" name="loanerP1" />

    <label for="loanerP2">Player 2 Loaners</label>
  <input type="text" value="{{circuit.loanerP2}}" name="loanerP2" />

    <label for="loanerP3">Player 3 Loaners</label>
  <input type="text" value="{{circuit.loanerP3}}" name="loanerP3" />

    <label for="loanerP4">Player 4 Loaners</label>
  <input type="text" value="{{circuit.loanerP4}}" name="loanerP4" />
{{!-- wins --}}
  <label for="winsP1">Player 1 Wins</label>
  <input type="text" value="{{circuit.winsP1}}" name="winsP1" />

  <label for="winsP2">Player 2 Wins</label>
  <input type="text" value="{{circuit.winsP2}}"name="winsP2" />

  <label for="winsP3">Player 3 Wins</label>
  <input type="text" value="{{circuit.winsP3}}" name="winsP3" />

  <label for="winsP4">Player 4 Wins</label>
  <input type="text" value="{{circuit.winsP4}}" name="winsP4" />
{{!-- loss --}}
