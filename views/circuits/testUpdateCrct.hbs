  <input type="text" value="{{circuit.lossP1}}" name="lossP1" />

    <label for="lossP2">Player 2 Losses</label>
  <input type="text" value="{{circuit.lossP2}}" name="lossP2" />

    <label for="lossP3">Player 3 Losses</label>
  <input type="text" value="{{circuit.lossP3}}" name="lossP3" />

   <label for="lossP4">Player 4 Losses</label>
  <input type="text" value="{{circuit.lossP4}}" name="lossP4" />
{{!-- total --}}
     <label for="totalP1">Player 1 Total</label>
  <input type="text" value="{{circuit.totalP1}}" name="totalP1" />

       <label for="totalP2">Player 2 Total</label>
  <input type="text" value="{{circuit.totalP2}}" name="totalP2" />

         <label for="totalP3">Player 3 Total</label>
  <input type="text" value="{{circuit.totalP3}}" name="totalP3" />

         <label for="totalP4">Player 4 Total</label>
